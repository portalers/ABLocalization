    # p "Here I go… All alone…" (cb_no_talk=True)
    p "我一個人……去囉……" (cb_no_talk=True)

# game/story/Chapter_L3.rpy:345
translate Tchinese chapter_L3_5ef264ad:

    # L "…." (cb_no_talk=True)
    L "……" (cb_no_talk=True)

