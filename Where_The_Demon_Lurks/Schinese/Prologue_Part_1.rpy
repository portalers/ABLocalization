    # game/story/Prologue_Part_1.rpy:101
    old "Is [name] correct?"
    new "确定是[name]吗？"

    # game/story/Prologue_Part_1.rpy:101
    old "Yes"
    new "是"

    # game/story/Prologue_Part_1.rpy:101
    old "No"
