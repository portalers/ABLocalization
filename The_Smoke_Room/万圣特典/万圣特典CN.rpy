    # m "\"I didn’t peg you as a guy who’s into elves.\""
    m "“没看出来你还对精灵感兴趣。”"

# game/halloween.rpy:2791
translate Schinese dressup_22cba2f4:

    # ni "\"It speaks to me Sam.\""
    ni "“它在对我说话，Sam。”"

# game/halloween.rpy:2794
translate Schinese dressup_b8168802:

    # ni "\"Listen...\""
    ni "“你听...”"

