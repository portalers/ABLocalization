translate Schinese dressup_eae72182:

    # m "\"...A pumber’s costume?\""
    m "“...水管工的打扮？”"

# game/halloween.rpy:2653
translate Schinese dressup_b473220a:

    # ni "\"Not just any pumber, Sam.\""
    ni "“可不是普普通通的水管工，Sam。”"

