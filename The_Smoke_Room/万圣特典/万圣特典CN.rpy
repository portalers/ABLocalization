translate Schinese dressup_f81fdc2d:

    # ni "\"Hrm.\""
    ni "“唔。”"

# game/halloween.rpy:2562
translate Schinese dressup_1904ee56:

    # ni "\"There are just so many US culture references that I don’t feel very confident with most of the characters.\""
