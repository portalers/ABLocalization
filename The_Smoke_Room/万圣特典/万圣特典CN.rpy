translate Schinese dressup_b386a6ef:

    # m "\"Maybe the generic costumes would be a better approach.\""
    m "“也许可以试试普通一点的衣服。”"

# game/halloween.rpy:2568
translate Schinese dressup_f0123cb1:

    # ni "\"My thoughts exactly...\""
    ni "“我也这样认为...”"

# game/halloween.rpy:2588
translate Schinese dressup_15a109db:

    # m "\"I feel like you manage to find the costumes that only make you look bigger.\""
    m "“这一身显得你更大了。”"

# game/halloween.rpy:2591
translate Schinese dressup_13dedb72:

    # ni "\"I’m dressed as a character from my favorite Columbian cartoon.\""
