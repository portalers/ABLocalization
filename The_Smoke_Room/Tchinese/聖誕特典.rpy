# game/script.rpy:174
translate Tchinese start_a99fb2f3:

    # m "\"It’s been Mariah fuckin’ Carey and Jingle Bell Rock on loop.\""
    m "“一直都是他媽的Mariah Carey跟Jingle Bell Rock循環播放。”"

# game/script.rpy:176
translate Tchinese start_c3915ae8:

    # wi "\"All she wants for Christmas is you, Sam.\""
    wi "“你是她唯一想要的聖誕禮物，Sam。”"

# game/script.rpy:178
translate Tchinese start_04f521bc:

    # m "\"Fuck you.\""
    m "“操你的。”"

# game/script.rpy:180
translate Tchinese start_4beeb1e4:

    # wi "\"Well, we are out of lube.\""
    wi "“可是我們潤滑用完了啊。”"

# game/script.rpy:182
translate Tchinese start_48f058eb:

    # wi "\"Might come in handy for later.\""
    wi "“等會買來備用好了。”"

# game/script.rpy:183
translate Tchinese start_b3f5e20b:

    # wi "\"I’ll put it on the fitbit.\""
    wi "“我先記在fitbit上。”"

# game/script.rpy:184
translate Tchinese start_1fcb70cd:

    # m "\"Doubt that’s necessary.\""
    m "“有這必要嗎？”"

# game/script.rpy:185
translate Tchinese start_e27941f9:

    # m "\"You don’t tend to forget things.\""
    m "“你又不常忘事。”"

# game/script.rpy:187
translate Tchinese start_08df81bf:

    # wi "\"Well I have one, so I’ll use it.\""
    wi "“既然我戴著，那就拿來用用。”"

# game/script.rpy:188
translate Tchinese start_d64d4a64:

    # m "\"You have a smartphone, too.\""
    m "“你也有手機啊。”"

# game/script.rpy:190
translate Tchinese start_0ab3479f:

    # wi "\"Fuck smartphones.\""
    wi "“去他的手機。”"

# game/script.rpy:191
translate Tchinese start_a62cc6b6:

