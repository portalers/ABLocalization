    # cl "“I think they look good!”"
    cl "“我覺得看起挺帥的呀！”"

# game/script.rpy:341
translate Tchinese start_3ec20bc0:

    # wi "“I don’t.”"
    wi "“我不覺得。”"

# game/script.rpy:344
translate Tchinese start_13545ba1:

    # to "“Well, at least HE has some taste!”"
    to "“哼，至少{b}他{/b}的品味不錯！”"

