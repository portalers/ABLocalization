# game/script.rpy:97
translate Tchinese start_c56ebf55:

    # m "“Why don’t {i}you{/i} call them?”"
    m "“{b}你{/b}打電話叫他們不就好了？”"

# game/script.rpy:100
translate Tchinese start_15a41b75:

    # wi "“Hrm...”"
    wi "“呃……”"

# game/script.rpy:102
translate Tchinese start_b8952ae6:

    # wi "“...Nah.”"
    wi "“……不要。”"

