translate Tchinese start_8c724ab5:

    # ni "“Snacks.”"
    ni "“零食。”"

# game/script.rpy:447
translate Tchinese start_97bfb737:

    # wi "“I always have food on hand, Nik.”"
    wi "“我手邊總有食物的，Nik。”"

# game/script.rpy:450
translate Tchinese start_07090e40:

    # ni "“Check! Check! Check!”"
    ni "“有！有！有！”"

